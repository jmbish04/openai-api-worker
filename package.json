{
  "name": "openai-api-worker",
  "version": "2.1.0",
  "description": "OpenAI-compatible API server using Cloudflare AI with multi-provider support",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npx wrangler dev",
    "deploy": "bash deploy.sh",
    "deploy:only": "npx wrangler deploy",
    "check-problems": "./check-problems.sh",
    "tail": "npx wrangler tail",
    "logs": "./logs.sh",
    "test": "./post-deploy-test.sh",
    "test:post-deploy": "echo 'Running post-deployment tests...' && npm run test:service-binding && npm run test:core-api",
    "test:service-binding": "node test/service-binding-test.js",
    "test:core-api": "node test/core-api-test.js",
    "deploy:staging": "npx wrangler deploy --env staging",
    "type-check": "tsc --noEmit -p .",
    "build": "tsc",
    "gemini-models": "tsx model_info/gemini-models.ts",
    "openai-models": "tsx model_info/openai-models.ts",
    "cloudflare-models": "tsx model_info/cloudflare-models.ts"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "openai",
    "ai",
    "api",
    "typescript",
    "gemini",
    "multi-provider"
  ],
  "author": "Colby",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "wrangler": "^4.37.1"
  },
  "dependencies": {
    "@google/genai": "^1.20.0",
    "openai": "^4.104.0"
  },
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531"
}
